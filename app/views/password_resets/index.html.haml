%h1 Listing password_resets

%table
  %tr
    %th User
    %th Token
    %th Datetime
    %th Used
    %th
    %th
    %th

  - @password_resets.each do |password_reset|
    %tr
      %td= password_reset.user
      %td= password_reset.token
      %td= password_reset.datetime
      %td= password_reset.used
      %td= link_to 'Show', password_reset
      %td= link_to 'Edit', edit_password_reset_path(password_reset)
      %td= link_to 'Destroy', password_reset, :method => :delete, :data => { :confirm => 'Are you sure?' }

%br

= link_to 'New Password reset', new_password_reset_path
